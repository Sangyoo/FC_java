package ch13;

import java.io.IOException;

public class SystemTest11 {

	public static void main(String[] args) {
		
		System.out.println("알파벳을 쓰고 enter를 누르세요");
		
		int i;
		
		try {
			i = System.in.read();
		} catch (IOException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
	}

}
